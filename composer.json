{
	"name": "vojtatranta/simple-annotations",
	"description": "Simple annotation parser",
	"homepage": "https://github.com/vojtatranta/php-simple-annotations",
	"keywords": ["annotations", "annotation", "docblock", "parser"],
	"license": "MIT",
	"authors":
	[
		{
			"name": "Jan Święcki",
			"email": "jan.swiecki@gmail.com",
			"role": "Developer"
		}
	],
	"require":
	{
		"php": ">=5.3.0"
	},
	"autoload":
	{
		"psr-0":
		{
			"DocBlockReader": "src/"
		},
		"psr-4": 
		{
            "DocBlockReader": "src/"
        },
        "classmap": ["src"]
	}
}